rule-configurations:
  - key: LONG_LINE
    value: 9999
  - key: MINIMUM_CASES_IN_SWITCH
    value: 1
disable-rules:
  - AssignIvarOutsideAccessors
  - ShortVariableName
  - LongVariableName
  - UnusedMethodParameter
  - PreferEarlyExit
  - TooManyMethods
  - UnnecessaryDefaultStatement
  - ConstantIfExpression
max-priority-1: 9999
max-priority-2: 9999
max-priority-3: 9999
enable-clang-static-analyzer: false
